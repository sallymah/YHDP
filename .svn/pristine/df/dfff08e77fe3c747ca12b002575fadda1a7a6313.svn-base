package tw.com.hyweb.core.yhdp.batch.framework.generic;

/**
 * @author Clare
 * 
 */
public class BatchJobException extends RcodeBaseException
{
    private static final long serialVersionUID = -4782488036136660904L;

    /**
     * @param throwable
     * @param rcode
     */
    public BatchJobException(Throwable throwable, String rcode)
    {
        super(throwable, rcode);
    }

    /**
     * @param message
     * @param rcode
     */
    public BatchJobException(String message, String rcode)
    {
        super(message, rcode);
    }
}
