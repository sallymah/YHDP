    }

    /**
     * checkCardNo getter
     * 
     * @return checkCardNo
     */
    public String getCheckCardNo()
    {
        return checkCardNo;
    }

    /**
     * checkCardNo setter
     * 
     * @param checkCardNo checkCardNo
     */
    public void setCheckCardNo(String checkCardNo)
    {
        this.checkCardNo = checkCardNo;
    }

    /**
     * checkCardStatus getter
     * 
