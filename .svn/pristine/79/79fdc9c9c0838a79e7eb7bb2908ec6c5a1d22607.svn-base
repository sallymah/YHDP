    /**
     * override equals method
     * 
     * @param o o
     * @return override
     */
    public boolean equals(Object o)
    {
        if (this == o)
        {
            return true;
        }
        if (o == null || getClass() != o.getClass())
        {
            return false;
        }

        CheckConditionInfo that = (CheckConditionInfo) o;

        if (checkBlacklist != null ? !checkBlacklist.equals(that.checkBlacklist) : that.checkBlacklist != null)
        {
            return false;
        }
        if (checkCardNo != null ? !checkCardNo.equals(that.checkCardNo) : that.checkCardNo != null)
        {
            return false;
        }
        if (checkCardStatusT != null ? !checkCardStatusT.equals(that.checkCardStatusT) : that.checkCardStatusT != null)
        {
            return false;
        }
        if (checkCardStatus != null ? !checkCardStatus.equals(that.checkCardStatus) : that.checkCardStatus != null)
        {
            return false;
        }
        if (checkMerch != null ? !checkMerch.equals(that.checkMerch) : that.checkMerch != null)
        {
            return false;
        }
        if (checkOrigTxn != null ? !checkOrigTxn.equals(that.checkOrigTxn) : that.checkOrigTxn != null)
        {
            return false;
        }
        if (checkTerm != null ? !checkTerm.equals(that.checkTerm) : that.checkTerm != null)
        {
            return false;
        }
        if (txnSrc != null ? !txnSrc.equals(that.txnSrc) : that.txnSrc != null)
        {
            return false;
        }

        return true;
    }

    /**
     * override hashCode method
