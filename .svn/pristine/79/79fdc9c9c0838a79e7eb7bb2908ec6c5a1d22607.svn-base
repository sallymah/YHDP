    public String toString()
    {
        StringBuffer sb = new StringBuffer();
        sb.append("[CheckConditionInfo: ");
        sb.append(" txnSrc:" + txnSrc);
        sb.append(" checkCardNo:" + checkCardNo);
        sb.append(" checkCardStatusT:" + checkCardStatusT);
        sb.append(" checkCardStatus:" + checkCardStatus);
        sb.append(" checkMerch:" + checkMerch);
        sb.append(" checkTerm:" + checkTerm);
        sb.append(" checkOrigTxn:" + checkOrigTxn);
        sb.append(" checkBlacklist:" + checkBlacklist);
        sb.append("]");
        return sb.toString();
    }
