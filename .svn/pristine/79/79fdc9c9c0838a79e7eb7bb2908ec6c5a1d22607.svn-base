     * @return override
     */
    public int hashCode()
    {
        int result;
        result = (txnSrc != null ? txnSrc.hashCode() : 0);
        result = 31 * result + (checkCardNo != null ? checkCardNo.hashCode() : 0);
        result = 31 * result + (checkCardStatus != null ? checkCardStatus.hashCode() : 0);
        result = 31 * result + (checkCardStatusT != null ? checkCardStatusT.hashCode() : 0);
        result = 31 * result + (checkMerch != null ? checkMerch.hashCode() : 0);
        result = 31 * result + (checkTerm != null ? checkTerm.hashCode() : 0);
        result = 31 * result + (checkOrigTxn != null ? checkOrigTxn.hashCode() : 0);
        result = 31 * result + (checkBlacklist != null ? checkBlacklist.hashCode() : 0);
        return result;
    }

